// Script 1 mail image animée domicile 

    // Attend que le DOM soit entièrement chargé avant d'exécuter le code
    document.addEventListener("DOMContentLoaded", function() {
        const animationFrames = document.querySelectorAll(".animation-frame");
        let currentFrameIndex = 0;

        // Vérifie si des éléments .animation-frame ont été trouvés
        if (animationFrames.length === 0) {
            console.error("Aucun élément .animation-frame trouvé.");
            return; // Arrête l'exécution du script
        }

        function changeImage() {
            animationFrames[currentFrameIndex].classList.add("hidden");
            currentFrameIndex = (currentFrameIndex + 1) % animationFrames.length;
            animationFrames[currentFrameIndex].classList.remove("hidden");
        }

        function startAnimation() {
            changeImage();
            setInterval(changeImage, 1000); // Change d'image toutes les secondes
        }

        startAnimation();
    });




